<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.kunli.una.mapper.SysSortMapper">
    <resultMap id="BaseResultMap" type="cn.kunli.una.pojo.system.SysSort">
        <id column="id" jdbcType="VARCHAR" property="id"/>
    </resultMap>
    <!--实体类数据库字段-->
    <sql id="columnName" >
        id
        ,name
        ,entity_id
        ,field_id
        ,sortord
        ,sort_order
        ,remark
        ,creator_id
        ,create_time
        ,modifier_id
        ,modify_time
        ,is_delete
        ,
    </sql>

    <!--实体类传值语句-->
    <sql id="columnValue" >
        #{record.id}
        ,#{record.name}
        ,#{record.entityId}
        ,#{record.fieldId}
        ,#{record.sortord}
        ,#{record.sort_order}
        ,#{record.remark}
        ,#{record.creatorId}
        ,#{record.createTime}
        ,#{record.modifierId}
        ,#{record.modifyTime}
        ,#{record.isDelete}

    </sql>
    <!--批量插入-->
    <insert id="batchInsert" parameterType="java.util.List">
        insert into sys_sort
        (<include refid="columnName" />)values
        <foreach collection="recordList" item="record" separator=",">
            (<include refid="columnValue" />)
        </foreach>
    </insert>

</mapper>
